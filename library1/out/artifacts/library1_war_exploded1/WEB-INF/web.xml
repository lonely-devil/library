<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    
    <welcome-file-list>
        <welcome-file>html/login.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/html/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>servlet.Home</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/html/home</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateData</servlet-name>
        <servlet-class>servlet.UpdateData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateData</servlet-name>
        <url-pattern>/html/updateData</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>selBook</servlet-name>
        <servlet-class>servlet.SelBook</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>selBook</servlet-name>
        <url-pattern>/html/selBook</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>username</servlet-name>
        <servlet-class>servlet.UpUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>username</servlet-name>
        <url-pattern>/html/user</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>exit</servlet-name>
        <servlet-class>servlet.Exit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>exit</servlet-name>
        <url-pattern>/html/exit</url-pattern>
    </servlet-mapping>
</web-app>